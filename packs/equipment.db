{"name":"Potion of Stamina","type":"equipment","img":"icons/svg/item-bag.svg","data":{"description":"This barely qualifies as a potion, as it resembles thick molasses, but it does convey a remarkable degree of toughness.","quantity":1,"eachweight":"","eachvalue":50,"slot":{"value":"noslot","slots":{"held":"Held","body":"Body","head":"Head","neck":"Neck","cloak":"Cloak","gloves":"Gloves","belt":"Belt","feet":"Feet","ring":"Ring","noslot":"None"}},"magical":false,"relatedprops":false,"props":"","critprops":"","relatedpower":true,"power":{"exists":false,"keywords":"Potion","action":"Minor Action","frequency":"Consumable","range":"Personal","targets":"","provoked":{"exists":false,"text":""},"requirement":{"exists":false,"text":""},"effect":{"exists":true,"text":"Until the end of your next turn, attack rolls vs. your Fortitude suffer disfavor."},"attack":{"exists":true,"bonus":"","hasthreat":false},"hit":{"text":""},"crit":{"text":""},"miss":{"text":""},"maintain":{"exists":false,"text":""},"special":{"exists":false,"text":""},"expended":false},"isarmor":false,"armor":{"acbonus":"","resist":"","encumbrance":"","speed":"","isheavy":false,"group":{"value":"","groups":{"cloth":"Cloth","leather":"Leather","chain":"Chain","scale":"Scale","plate":"Plate"}}},"isshield":false,"shield":{"defbonus":"","encumbrance":"","speed":"","dice":"","attack":{"feat":0,"itemb":0,"misc":0},"damage":{"feat":0,"itemb":0,"misc":0},"group":{"value":"","groups":{"lshield":"Light Shield","hshield":"Heavy Shield"}}},"isweapon":false,"weapon":{"mvsr":{"value":"","mvsrs":{"melee":"Melee","ranged":"Ranged"}},"complexity":{"value":"","complexities":{"improvised":"Improvised","simple":"Simple","military":"Military"}},"hands":{"value":"","handses":{"lt":"Light","1h":"One-handed","2h":"Two-handed"}},"quantity":1,"weight":"","prof":"","dice":"","range":"","groups":"","value":"","quals":{"bayonet":false,"bulky":false,"critpotential":false,"critthreat":false,"heavythrown":false,"lightthrown":false,"load":false